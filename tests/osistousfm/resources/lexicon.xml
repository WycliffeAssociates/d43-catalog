	<entry id="H1">
		<w pos="n-m" pron="awb" xlit="ʼâb" xml:lang="heb">אָב</w>
		<source>a primitive word;</source>
		<meaning><def>father</def>, in a literal and immediate, or figurative and remote application</meaning>
